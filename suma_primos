#Start code here
num = 2
suma = 0
bucle = 0
while bucle < 20:
  primo = True
  div = 2
  while div < num:
    if num % div == 0:
      primo = False
      break
    div = div + 1
  if primo == True:
    suma = suma + num
    bucle = bucle + 1
    print(num, " es número primo") # your own code
  num = num + 1
print("La suma de los 20 primeros números primos es", suma) # your own code
